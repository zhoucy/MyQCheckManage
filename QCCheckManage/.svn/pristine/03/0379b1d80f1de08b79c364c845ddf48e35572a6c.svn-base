@charset "utf-8";
*{font-family:"Microsoft YaHei","微软雅黑",Verdana,Arial,Helvetica,sans-serif;}
body{margin:0; color:#666; font-family:"Microsoft YaHei","微软雅黑"; font-size:14px;}
ul{margin:0; padding:0; list-style:none;}
dl{margin:0;}
dd{margin:0;}
a{color:inherit; cursor:pointer; text-decoration:none;}
em{font-style:normal;}
label{margin-right:8px;}
.select-1{  overflow:hidden; margin-right:30px; border:1px solid #e3e3e3;}
/*全局类*/
.container{overflow:hidden; background-color:#fff; min-width:120px;}
.red-info{color:#FF0004;}  /*静态红字*/
.input-tip{color:#FF0004;}  /*动态红字*/
.blue-info{color:#3277de;}
.blue-info{color:#3277de; font-size: 18px;}
.green-info{color:green;}
.light-blue-text{color: #7aa9ec;}
.gray-text{color: #999;}

.WhiteBig-btn{font-size:16px; line-height:16px; font-weight:bold; text-align:center; padding:12px 28px; border-radius:3px; margin-top:22px; display:inline-block; background-color:#fff; border:1px #e3e3e3 solid;}
.WhiteBig-btn:hover{background-color:#f7f7f7;}
.BlueBig-btn{font-size:16px; line-height:16px; font-weight:bold; text-align:center; padding:12px 28px; border-radius:3px; margin-top:22px; display:inline-block; background-color:#3277de; color:#fff;}
.BlueBig-btn:hover{background-color:#478ffb; border:1px #478ffb solid;}
.BlueBig-btn:active{background-color:#1C59B3;}
.big-subBtn-white{font-size:16px; padding:10px 30px; background-color:#fff; text-align:center; border:1px #e3e3e3 solid; border-radius:4px; font-weight:bold; display:inline-block;}
.big-subBtn-white:hover{background-color:#f7f7f7;}
.big-subBtn-Blue{font-size:16px; padding:10px 30px; background-color:#3277de; text-align:center; border:1px #3277de solid; border-radius:4px; font-weight:bold; color:#fff !important; display:inline-block;}
.big-subBtn-Blue:hover{background-color:#478ffb; border:1px #478ffb solid;}
.big-subBtn-Blue:active{background-color:#1C59B3;}
.big-subBtn-gray{font-size:16px; padding:10px 30px; background-color:#bbbbbb; text-align:center; border:1px #bbbbbb solid; border-radius:4px; font-weight:bold; color:#fff !important; display:inline-block;}
.big-subBtn-gray:hover{background-color:#c7c7c7; border:1px #c7c7c7 solid;}
.big-subBtn-white-2{font-size:16px; padding:10px 30px; background-color:#fff; text-align:center; border:1px #e3e3e3 solid; border-radius:4px; display:inline-block;}
.big-subBtn-white-2:hover{background-color:#f7f7f7;}
.big-subBtn-Blue-2{font-size:16px; padding:10px 30px; background-color:#3277de; text-align:center; border:1px #3277de solid; border-radius:4px; color:#fff !important; display:inline-block;}
.big-subBtn-Blue-2:hover{background-color:#478ffb; border:1px #478ffb solid;}
.big-subBtn-Blue-2:active{background-color:#1C59B3;}
.big-subBtn-gray-2{font-size:16px; padding:10px 30px; background-color:#bbbbbb; text-align:center; border:1px #bbbbbb solid; border-radius:4px; color:#fff !important; display:inline-block;}
.big-subBtn-gray-2:hover{background-color:#c7c7c7; border:1px #c7c7c7 solid;}
.a-btn{line-height:1; padding:8px 20px; border:1px #e3e3e3 solid; border-radius:3px; background-color:#fff; display:inline-block;}
.a-btn:hover{background-color:#f7f7f7;}
.a-btn-bold{font-weight:bold; padding:8px 20px; border:1px #e3e3e3 solid; border-radius:3px; background-color:#fff; display:inline-block;}
.a-btn-bold:hover{background-color:#f7f7f7;}
.a-blueText{color:#3277de;}
.a-btnBlue{color:#fff; line-height:1; padding:8px 20px; border:1px #3277de solid; border-radius:3px; background-color:#3277de; display:inline-block;}
.a-btnBlue:hover{background-color:#478ffb; border:1px #478ffb solid;}
.a-btnBlue:active{background-color:#1C59B3;}
.a-grayText{color:#666; margin-right:14px;}

.save-bar{text-align:center; margin:50px auto; overflow:hidden;}
.textInput{height:30px; line-height:30px; border:1px #e3e3e3 solid; background-color:#fff; text-indent:8px; color:#666; margin-right: 10px;}
.textArea{border:1px #e3e3e3 solid; min-width:320px; min-height:144px;}
.select{height:32px; line-height:32px; border:1px #e3e3e3 solid; background-color:#fff; text-indent:8px; color:#666; margin-right: 10px;}
.select option{color:#000;}
.inputStatus{height:30px; line-height:30px; float:right;}
.border-container{overflow:hidden; border:1px #e3e3e3 solid;}
.section-title{height:40px; line-height:40px; background-color:#f6f6f6; text-indent:30px; font-size:16px; color:#333;}
.left-blue{width:8px; height:40px; background-color:#3277de; float:left;}

.table-head-sanjiao img{margin-left: 8px; cursor: pointer;}

.table50{width:100%; border:1px #e3e3e3 solid; border-collapse:collapse;}
.table50 th{height:36px; background:url(../images/tableLine.png) right no-repeat; font-weight:normal;}
.table50 th:nth-last-child(1){background-image:none;}
.table50 tbody tr td{padding:13px 0; text-align:center; color:#333; word-wrap:break-word;}
.table50 tbody tr:nth-child(odd){background-color:#f7f7f7;}

.table40{width:100%; border:1px #e3e3e3 solid; border-collapse:collapse;}
.table40 th{height:36px; background:url(../images/tableLine.png) right no-repeat; font-weight:normal;}
.table40 th:nth-last-child(1){background-image:none;}
.table40 tbody tr td{padding:10px 0; text-align:center; color:#333; word-wrap:break-word;}
.table40 tbody tr:nth-child(odd){background-color:#f7f7f7;}

.table{border:1px #e3e3e3 solid; border-collapse:collapse;}
.table th{height:36px; background:url(../images/tableLine.png) right no-repeat; font-weight:normal;}
.table th:nth-last-child(1){background-image:none;}
.table tbody tr:nth-child(odd){background-color:#f7f7f7;}
.table tbody tr td{line-height:50px; text-align:center;}
.table tbody tr td input[type="text"]{width:85%; height:28px; border:1px #e3e3e3 solid; text-align:center; color:#333;}

.border-table{border:1px #e3e3e3 solid; border-collapse:collapse;}
.white-table-head{height:34px; background-color:#fff;}
.white-table-head td{line-height:34px; text-align:center; background:url(../images/tableLine.png) right no-repeat;}
.white-table-head td:nth-last-child(1){ background-image:none;}
.gray-table-input{height:50px; background-color:#f7f7f7;}
.gray-table-input td{line-height:50px; text-align:center;}
.simple-table{border:1px #e3e3e3 solid; background-color:#fff; border-collapse:collapse;}
.simple-table td{border:1px #e3e3e3 solid; height:40px; line-height:40px; text-align:center;}

/*头部*/
.g-header{min-width:1200px; height:60px; background-color:#fff;}
.g-header div{height:60px; line-height:60px;}
.g-header em{color:#E7292C; font-style:normal; margin:0 8px;}
.g-logo{width:100px; float:left;}
.g-top-title{min-width:200px; float:left; color:#3277de; font-size:20px; padding-left:15px;}
.g-top-info{width:490px; float:right; font-size:14px; color:#555;}
.g-top-info span,.top-info a{float:right; margin-right:20px;}

/*标签卡*/
.tab-item-box{width:100%; height:38px; border-bottom:#3277de 2px solid; padding-left:20px;}
.tab-item{display:inline-block; width:112px; heihgt:38px; font-size:16px; line-height:38px; text-align:center; font-weight:bold; position:relative; bottom:-2px;}
.tab-item-hover{color:#3277de;}
.tab-item-active{color:#3277de; background:url(../../images/topBlue.png) bottom no-repeat;}
.tab-item-active2{color:#3277de; border-bottom:2px #3277de solid; font-weight:bold;}

/*标签附加*/
.Order-Screening{height:38px; line-height:38px; min-width:1100px; border-bottom:2px #e3e3e3 solid; margin-top:50px; margin-bottom:20px;}
.switchLine-tab li{width:110px; height:38px; line-height:38px; text-align:center; float:left; position:relative; margin-bottom:-2px;}

/*子标签卡*/
.subTab-item-box{height: 32px; border-bottom: 2px #e3e3e3 solid;}
.subTab-item{font-weight: bold; line-height: 32px; text-align: center; padding: 0 24px; display: inline-block; float: left; position: relative;}
.subTab-item-active{color: #3277de; border-bottom: 2px #3277de solid;}

/*子标签卡-大*/
.subTab-itemSmall-Bar{width:100%; height:35px; border-bottom:2px #e3e3e3 solid; margin:14px auto;}
.subTab-itemSmall{width:100%; height:35px; padding:0 11px; display:block; position:relative;}
.subTab-itemSmall li{font-size:15px; height:33px; line-height:33px; padding:0 24px; border-bottom: 2px transparent solid; color:#666; display: inline-block; float: left; position: relative; bottom:-2px; overflow:hidden;}
.subTab-itemSmall li a{width:100%; height:100%; display:inline-block;}
.subTab-itemSmall-active{color: #3277de !important; border-bottom: 2px #3277de solid !important;}
.subTab-itemSmall-left{display:inline-block; width:11px; height:24px; position:absolute; left:0; top:50%; margin-top:-12px; background:url(../images/fans-img/tab-btn1.png) left no-repeat;}
.subTab-itemSmall-leftAvailable{ background:url(../images/fans-img/tab-btn2.png) left no-repeat;}
.subTab-itemSmall-right{display:inline-block; width:11px; height:24px; position:absolute; right:0; top:50%; margin-top:-12px; background:url(../images/fans-img/tab-btn2.png) right no-repeat;}
.subTab-itemSmall-rightAvailable{ background:url(../images/fans-img/tab-btn1.png) right no-repeat;}

/*主体内容*/
.g-main-body{margin-left:20px; min-width:900px;}

.g-main-tab{height:42px; padding-left:30px;}
.g-tab{width:82px; height:34px; margin-top:8px; float:left; font-size:16px; font-weight:bold; text-align:center; line-height:34px; background-color:#f2f2f2; border-radius:5px 5px 0 0;}
.g-tab-active{color:#3277de; background-color:#fff;}
.g-right-tab{width:204px; height:42px; float:right; font-size:14px; line-height:42px; color:#666;}
.g-right-tab a{height:16px; border-right:1px #AFAFAF solid; padding:0 5px;}
.g-main{min-height:828px; padding-left:160px; padding-right:20px; background-color:#fff; position:relative; overflow:hidden;}
.g-left{width:150px; height:100%; padding:20px 18px; background-color:#fff; position:absolute; left:0;}
.g-left dl{width:150px; font-size:18px; line-height:2.5; display:block;}
.g-left dl dt{width:124px; height:20px; line-height:20px; padding:12px 0; border-bottom:1px #ccc solid; margin-bottom:5px; display:table-cell; color:#333;}
.g-left dl dt img{height:20px; margin-right:5px; float:left;}
.g-left dl dd{margin-left:26px;}
.g-left dl dd:hover{color:#ed1d30;}
.g-currentMenu{color:#3277de;}
.g-left-blue{color:#3277de;}
.g-right{width:100%; min-width:1200px; padding-top:23px; float:left; overflow:hidden;}

/*翻页*/
.g-pageBar{height:38px; min-width:870px; margin-top:20px; margin-bottom:50px;}
.g-pages{min-width:870px; width:auto; height:38px; line-height:38px; margin:auto;}
.g-pages div{height:38px; float:left; margin-right:10px;}
.g-pages input{width:50px; height:38px; background-color: #fff; border:1px #e3e3e3 solid; border-radius:3px; text-align:center;}
.g-pages input[type="text"]{padding:0; height:36px;}
.g-pages input[type="submit"]:hover{background-color: #3277de; color:#fff;}
.g-pages label{line-height:38px; margin-right:8px;}
.g-pageNumList{display:inline;}
.g-pageNumList li{display:inline; width:60px; height:36px; background-color: #fff; border:1px #e3e3e3 solid; border-radius:3px; float:left; margin-right:4px; text-align:center; line-height: 36px;}
.g-pageNum-long{width:60px !important;}
.g-pageNumList li:hover{background-color: #3277de; color:#fff;}
.g-pageNumList li a{display:block;width:100%;height:100%}
.g-aitve{background-color: #3277de !important; color:#fff;}/*选中style*/
.g-disable{background-color: #FFF !important; color:#ccc;}

/*底部版权*/
.g-footer{width:100%; height:48px; color:#9c9c9c; font-size:12px; line-height:48px; text-align:center; border-top:1px #ccc solid; background-color:#fff;}
.g-footer-support{width:100%; height:48px; color:#9c9c9c; font-size:12px; text-align:center; background-color:#fff;}