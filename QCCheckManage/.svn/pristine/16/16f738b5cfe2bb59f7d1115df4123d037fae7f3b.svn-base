/*弹出框的共用*/
.g-dialog-bg{position: fixed;width: 100%;height: 100%;top: 0px;left: 0px;opacity: 0.2;z-index: 9999;background: rgb(0, 0, 0);}
.g-dialog{opacity: 1;z-index: 9999;transform: scale(1);transition: -webkit-transform 0.2s;position: absolute; background: #fff;}
.g-dialog .g-dialog-tit{height: 40px;line-height: 40px;background: #3277de;color: #fff;font-size: 16px;padding: 0 0 0 15px;position: relative;}
.g-dialog .g-dialog-tit .g-dialog-close {display: block;position: absolute;width: 40px;height: 40px;top: 0;right: 4px;background:url(../images/dialog_tit_x.png);cursor: pointer;}

